@model Sitecore.GnosisToolkit.Areas.GnosisToolkit.Models.Layouts.BasicHtml

<html lang="@Model.Lang">
<head>
    <meta charset="utf-8"/>
    <link rel="canonical" href="@Model.CanonicalUrl"/>
    @if (Model.ShowFavicon)
    {
        <link rel="icon" type="@Model.FaviconMimeType" href="@Model.FaviconPath"/>
    }
    <!--head start-->
    @Html.Sitecore().Placeholder("head")
    <!--head end-->
</head>
<body>
    <!--body start-->
    @Html.Sitecore().Placeholder("body")
    <!--body end-->
</body>
</html>
